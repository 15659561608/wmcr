<script type="text/javascript" src="lib/datatables/1.10.0/jquery.dataTables.min.js"></script>

<script src="../layui/layui.all.js"></script>

<script type="text/javascript">
/*管理员-角色-添加*/
function admin_role_add(title,url,w,h){
	layer_show(title,url,w,h);
}
/*管理员-角色-编辑*/
function admin_role_edit(title,url,id,w,h){
	layer_show(title,url,w,h);
}
/*管理员-角色-删除*/
function admin_role_del(obj,id){
	layer.confirm('角色删除须谨慎，确认要删除吗？',function(index){
		$.ajax({
			type: 'POST',
			url: '',
			dataType: 'json',
			success: function(data){
				$(obj).parents("tr").remove();
				layer.msg('已删除!',{icon:1,time:1000});
			},
			error:function(data) {
				console.log(data.msg);
			},
		});		
	});
}
</script>


<script type="text/html" id="toolbarDemo">
  <div class="layui-btn-container">
    <button class="layui-btn layui-btn-sm" lay-event="getCheckData">获取选中行数据</button>
    <button class="layui-btn layui-btn-sm" lay-event="getCheckLength">获取选中数目</button>
    <button class="layui-btn layui-btn-sm" lay-event="isAll">验证是否全选</button>
  </div>
</script>
<script type="text/html" id="barDemo">
  <a class="layui-btn layui-btn-xs" lay-event="edit">编辑</a>
  <a class="layui-btn layui-btn-danger layui-btn-xs" lay-event="del">删除</a>
</script>
              

<script>
layui.use('table', function(){
  var table = layui.table;
  
  table.render({
    elem: '#test'
    ,url:'../Business'
    ,toolbar: '#toolbarDemo'
    ,title: '门店数据表'
    ,totalRow: true
    
    ,cols: [[
      {type: 'checkbox', fixed: 'left'}
      ,{field:'busiName', title:'门店名', width:150, fixed: 'left', unresize: true, sort: true, totalRowText: '合计'}
    ,{field:'phone', title:'联系电话', width:150, edit: 'text'}
      ,{field:'address', title:'地址', width:150, edit: 'text', 
      }
      ,{field:'avgCost', title:'平均消费', width:100, sort: true, totalRow: true}
      ,{field:'licence', title:'营业执照', width:150, edit: 'text', sort: true}
      ,{field:'salNum', title:'销量', width:100, sort: true, totalRow: true}
      ,{field:'startPrice', title:'起送价',width:100}
      ,{field:'disFee', title:'配送费', width:100}
      ,{field:'onlinePay', title:'在线支付', width:120}
      ,{field:'isReserve', title:'支持预定', width:120}
      ,{field:'isBusiness', title:'是否预定', width:120}
    ]]
    ,page: true
  });
  
  //工具栏事件
  table.on('toolbar(test)', function(obj){
    var checkStatus = table.checkStatus(obj.config.id);
    switch(obj.event){
      case 'getCheckData':
        var data = checkStatus.data;
        layer.alert(JSON.stringify(data));
      break;
      case 'getCheckLength':
        var data = checkStatus.data;
        layer.msg('选中了：'+ data.length + ' 个');
      break;
      case 'isAll':
        layer.msg(checkStatus.isAll ? '全选': '未全选')
      break;
    };
  });
});
</script>

